    "scripts": {
      "start": "craco start",
      "build": "craco build",
      "test": "craco test",
      "stylelint:fix": "yarn stylelint --fix",
      "eslint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix --cache",
      "stylelint": "stylelint 'src/**/*.{css,less}' --custom-syntax postcss-less",
      "eslint": "eslint src --ext .js,.jsx,.ts,.tsx",
      "lint": "yarn stylelint -f json > stylelint-report.json && yarn eslint -f json -o eslint-report.json",
      "postinstall": "husky install && yarn husky:init",
      "husky:init": "husky set .husky/prepare-commit-msg 'exec < /dev/tty && ./node_modules/.bin/cz --hook || true'&& husky set .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'&& husky set .husky/pre-commit 'npx lint-staged'"
    }
  }
